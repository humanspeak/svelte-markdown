{
    "name": "docs",
    "version": "0.0.1",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "vite build",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "deploy": "npm run build && wrangler pages deploy",
        "dev": "vite dev",
        "format": "prettier --write .",
        "lint": "prettier --check . && eslint .",
        "lint:fix": "npm run format && eslint . --fix",
        "preview": "vite preview",
        "sitemap:manifest": "node ./scripts/generate-sitemap-manifest.mjs",
        "sitemap:watch": "chokidar 'src/routes/**/*.svelte' 'src/routes/**/*.svx' 'src/routes/**/*.md' --ignore 'src/routes/examples/+page.ts' -c 'node ./scripts/generate-sitemap-manifest.mjs' --initial --silent"
    },
    "dependencies": {
        "@humanspeak/svelte-markdown": "workspace:*",
        "github-slugger": "^2.0.0",
        "runed": "^0.37.1"
    },
    "devDependencies": {
        "@cloudflare/workers-types": "^4.20260218.0",
        "@eslint/compat": "^2.0.2",
        "@eslint/js": "^10.0.1",
        "@humanspeak/svelte-motion": "^0.1.21",
        "@sveltejs/adapter-cloudflare": "^7.2.8",
        "@sveltejs/kit": "^2.52.2",
        "@sveltejs/vite-plugin-svelte": "^6.2.4",
        "@tailwindcss/postcss": "^4.2.0",
        "@tailwindcss/typography": "^0.5.19",
        "@tailwindcss/vite": "^4.2.0",
        "@typescript-eslint/eslint-plugin": "^8.56.0",
        "@typescript-eslint/parser": "^8.56.0",
        "autoprefixer": "^10.4.24",
        "chokidar-cli": "^3.0.0",
        "eslint": "^10.0.0",
        "eslint-config-prettier": "10.1.8",
        "eslint-plugin-svelte": "3.15.0",
        "globals": "^17.3.0",
        "lucide-svelte": "^0.574.0",
        "mdsvex": "^0.12.6",
        "mode-watcher": "^1.1.0",
        "prettier": "^3.8.1",
        "prettier-plugin-organize-imports": "^4.3.0",
        "prettier-plugin-sort-json": "^4.2.0",
        "prettier-plugin-svelte": "^3.5.0",
        "prettier-plugin-tailwindcss": "^0.7.2",
        "shiki": "^3.22.0",
        "svelte": "^5.53.0",
        "svelte-check": "^4.4.1",
        "tailwind-merge": "^3.5.0",
        "tailwindcss": "^4.2.0",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.56.0",
        "vite": "^7.3.1",
        "wrangler": "^4.66.0"
    },
    "volta": {
        "node": "24.13.0"
    }
}
